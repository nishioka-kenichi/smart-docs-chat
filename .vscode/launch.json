{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Phase03: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "python": "${workspaceFolder}/phase03-langgraph/.venv/bin/python",
            "cwd": "${workspaceFolder}/phase03-langgraph",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/phase03-langgraph"
            }
        },
        {
            "name": "Phase02: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "python": "${workspaceFolder}/phase02-advanced-rag/.venv/bin/python",
            "cwd": "${workspaceFolder}/phase02-advanced-rag",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/phase02-advanced-rag"
            }
        },
        {
            "name": "Phase01: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "python": "${workspaceFolder}/phase01-local/.venv/bin/python",
            "cwd": "${workspaceFolder}/phase01-local",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/phase01-local"
            }
        },
        {
            "name": "Phase02: HyDE Test",
            "type": "debugpy",
